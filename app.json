{
  "expo": {
    "name": "appclip-demo",
    "slug": "appclip-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.timmyjose.appclipdemo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.timmyjose.appclipdemo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-build-properties",
      [
        "react-native-app-clip",
        {
          "name": "appclip-demo clip",
          "bundIdSuffix": "Clip",
          "targetSuffix": "Clip",
          "groupIdentifier": "com.timmyjose.appclipdemo",
          "enabled": true,
          "appleSignin": true,
          "requestEphemeralUserNotification": true,
          "excludedPackages": [],
          "pushNotifications": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5f21d08a-881e-460c-9917-00a67fdcd666"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5f21d08a-881e-460c-9917-00a67fdcd666"
    }
  }
}
